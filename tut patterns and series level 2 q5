#include <iostream>
using namespace std;
int power(int a,int b)
{
    if(b==0) return 1;
    else return a*power(a,b-1);
}
int main()
{
    int n,i,j,t;
    cin>>t;
    while(t--)
    {
    cin>>n;
    if(n==1||n==2) cout<<"1"<<endl;
    if(n==3) cout<<"2"<<endl;
    if(n==4) cout<<"3"<<endl;
    if(n>4)
    {
        n=n-4;
        if(n%2)
        {
            j=2;
            for(i=n;i>=0;i=i-2)
            {
                j=j*2;
            }
            cout<<j<<endl;
        }
        else
        {
            j=3;
            for(i=n;i>0;i=i-2)
            {
                j=j*3;
            }
            cout<<j<<endl;
        }
    }
    }
}
