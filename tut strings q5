#include <iostream>
#include <string.h>
using namespace std;
int main()
{
    char a[100];
    cin>>a;
    int i,len=strlen(a),f=0;
    //if(i%2==0)
    //{
        for(i=0;i<len/2;i++)
        {
            f=0;
            if(a[i]!=a[len-1-i])
            {
                f=1;cout<<"not a palindrome\n";break;
            }
        }
        if(f==0) cout<<"palindrome";
}
